{
  "name": "macro-restuarant",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "node-sass sass/main.scss style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch",
    "compile": "node-sass sass/main.scss style.comp.css ",
    "concat": "concat -o style.concat.css style.comp.css",
    "prefix": "postcss --use autoprefixer -b 'last 10 versions' style.concat.css -o style.prefix.css",
    "compress": "node-sass style.prefix.css style.css --output-style compressed",
    "build": "npm-run-all compile concat prefix compress"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0"
  },
  "dependencies": {
    "live-server": "^1.2.2"
  }
}